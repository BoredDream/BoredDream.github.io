<script>
// 增强favicon加载处理
document.addEventListener('DOMContentLoaded', function() {
  const favicons = document.querySelectorAll('.link-favicon');
  
  favicons.forEach(favicon => {
    // 检查图片是否已经加载失败
    if (favicon.complete && favicon.naturalHeight === 0) {
      // 图片加载失败
      favicon.style.display = 'none';
      const noFavicon = favicon.nextElementSibling;
      if (noFavicon && noFavicon.classList.contains('no-favicon')) {
        noFavicon.classList.add('show');
      }
    }
    
    // 添加加载成功监听
    favicon.addEventListener('load', function() {
      this.style.display = 'block';
      const noFavicon = this.nextElementSibling;
      if (noFavicon && noFavicon.classList.contains('no-favicon')) {
        noFavicon.classList.remove('show');
      }
    });
    
    // 添加加载失败监听（备用）
    favicon.addEventListener('error', function() {
      this.style.display = 'none';
      const noFavicon = this.nextElementSibling;
      if (noFavicon && noFavicon.classList.contains('no-favicon')) {
        noFavicon.classList.add('show');
      }
    });
  });
});
</script>
